<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:context="http://www.springframework.org/schema/context"
        xsi:schemaLocation="http://www.springframework.org/schema/beans
            http://www.springframework.org/schema/beans/spring-beans.xsd
            http://www.springframework.org/schema/context
            http://www.springframework.org/schema/context/spring-context.xsd">

   <bean id="ab" class="org.jsp.app.University" autowire="byType">
   		<property name="uName" value="JNTU"></property>
   		<property name="uAddress" value="Kukatpally"></property>
   </bean>

	<bean id="cd" class="org.jsp.app.College" autowire="byType">
			<property name="cname" value="CDC"></property>
			<property name="cid" value="CITS"></property>
	</bean>
	
	<bean id="ef" class="org.jsp.app.Department" autowire="byType">
			<property name="dName" value="CSE"></property>
			<property name="dHname" value="ragu"></property>
	</bean>
	
	<bean id="gh" class="org.jsp.app.Faculty" autowire="byType">
			<property name="fName" value="Ravi"></property>
			<property name="fId" value="nine"></property>
	</bean>
	
</beans>