package org.jsp.app;

public class alp 
{
	public static void main(String[] args) 
	{
		String s1="abcdef";
		String s2=" ";
		
		for(int i=0;i<s1.length()-1;i++)
		{
			s2=s2+s1.charAt(i);
		}
		
		if ((s2=="a") || (s2=="e") || (s2=="e") || (s2=="i") || (s2=="o") || (s2=="u"))
		{
			System.out.println(s2);
		}
		else
		{
			System.out.println("not a vowel");
		}
		
		
	}
}
		   