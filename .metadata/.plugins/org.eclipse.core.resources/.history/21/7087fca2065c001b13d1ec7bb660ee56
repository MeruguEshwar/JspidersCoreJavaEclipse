package Forloop;

public class Prime 
{
	public static void main(String[] args) 
	{
		int n=7;
		int count=0;
		for(int i=0;i<=7;i++)
		{
			if(n%i==0)
			{
				count++;
			}
		}
			if(count==2)
			{
				System.out.println("The given number is Prime...");
			}
			else
			{
				System.out.println("Not a Prime.................");
			}
		}
	}
}
