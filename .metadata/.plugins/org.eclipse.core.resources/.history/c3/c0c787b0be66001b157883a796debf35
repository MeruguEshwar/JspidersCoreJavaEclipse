package org.jsp.app;

public class alp 
{
	public static void main(String[] args) 
	{
		String s1="abcdef";
		String s2=" ";
		
		if ((s1=="a") || (s1=="e") || (s1=="e") || (s1=="i") || (s1=="o") || (s1=="u"))
		{
			s2=s2+s1;
			System.out.println(s2);
		}
		else
		{
			System.out.println("not a vowel");
		}
		
		
	}
}
		   