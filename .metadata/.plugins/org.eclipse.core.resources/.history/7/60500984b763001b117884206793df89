/*Wap to print total occurrence of the given element in an array*/
package Array;

public class Program11 
{
	public static void main(String[] args) 
	{
		int [] a1= {23,45,67,45,12,34,45,23,98};
		int givenElement = 45;
		int count = 0;
		
		for(int i=0;i<=a1.length-1;i++)
		{
			if(givenElement == a1[i])
			{
				count++;
			}
		System.out.println("The number of occerence of elemt is"+a1[i]);
		}
	}
}
