/*Wap to print total occurrence of the given element in an array*/
package Array;

public class Program11 
{
	public static void main(String[] args) 
	{
		int [] a1= {23,45,67,45,12,34,45,23,98};
		int len= a1.length;
		int temp[]=new int[a1.length];
		int j=0;
		
		for(int i=0;i<a1.len-1;i++)
		{
			if(a1[i] != a1[i+1])
			{
				temp[j++]=a1[i];
			}
		}
		
		temp[j++] = a1[length-1];
		System.out.println(temp[j]);
	}
}
