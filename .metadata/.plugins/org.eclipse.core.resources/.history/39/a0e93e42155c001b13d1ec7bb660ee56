package Forloop;

public class Patrn4 
{
	public static void main(String[] args) 
	{
		System.out.print("Hii nani...");
		Patrn4 obj=new Patrn4();
		obj.mammu();
	}
	public static void add(int i,int j)
	{
		int c=i+j;
		System.out.print(c);
	}
	public static void add(int i,int j,int k)
	{
		int a=i+j+k;
		System.out.print(a);
	}
}
